@model Specification

@{
    ViewData["Title"] = "Create";
}

<div class="page-header">
	<div class="row align-items-end">
		<div class="col-lg-8">
			<div class="page-header-title">
				<div class="d-inline">
					<h4>Specification</h4>
					<span>
						Create new Specification
					</span>
				</div>
			</div>
		</div>
		<div class="col-lg-4">
			<div class="page-header-breadcrumb">
				<ul class="breadcrumb-title">
					<li class="breadcrumb-item" style="float: left;">
						<a asp-area="admin" asp-controller="dashboard" asp-action="index"> <i class="feather icon-home"></i> </a>
					</li>
					<li class="breadcrumb-item" style="float: left;">
						<a href="#!">Specification</a>
					</li>
				</ul>
			</div>
		</div>
	</div>
</div>
<div class="card">
	<div class="card-block">
		<form method="post">
			<div class="form-group row">
				<label asp-for="Name" class="col-sm-2 col-form-label"></label>
				<div class="col-sm-10">
					<input asp-for="Name" class="form-control">
					<span asp-validation-for="Name" class="messages"></span>
				</div>
			</div>
			<div class="form-group row">
				<label class="col-sm-2"></label>
				<div class="col-sm-10">
					<button type="submit" class="btn btn-success m-b-0">Submit</button>
					<a asp-action="index" class="btn btn-primary m-b-0">Go To List</a>
				</div>
			</div>
		</form>
	</div>
</div>

